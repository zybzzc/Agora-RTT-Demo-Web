{
  "version": 2,
  "buildCommand": "yarn build",
  "outputDirectory": "dist",
  "installCommand": "yarn install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/((?!assets|locales|.*\\.).*)$",
      "destination": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 